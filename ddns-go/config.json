{
  "apparmor": true,
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
  "description": "filebrowser provides a file managing interface within a specified directory and it can be used to upload, delete, preview, rename and edit your files",
  "ingress": true,
  "ingress_port": 9876,
  "ingress_stream": true,
  "map": [
    "media:rw",
    "config:rw",
    "addons:rw",
    "backup:rw",
    "share:rw",
    "ssl"
  ],
  "name": "ddns-go",
  "options": {
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "NoAuth": true
  },
  "panel_icon": "mdi:file-search",
  "ports": {
    "9876/tcp": 9876
  },
  "ports_description": {
    "9876/tcp": "Web UI port"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str"
  },
  "slug": "filebrowser",
  "startup": "services",
  "upstream": "4.2.0",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "4.2.0",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:9876]"
}
