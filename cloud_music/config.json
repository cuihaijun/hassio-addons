{
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "description": "cloud music with ease",
  "docker_api": true,
  "hassio_api": true,
  "ingress": true,
  "ingress_port": 1337,
  "ingress_stream": true,
  "init": false,
  "map": [
    "share:rw",
    "ssl"
  ],
  "name": "cloud_music",
  "boot": "auto",
  "image": "binaryify/netease_cloud_music_api",
  "options": {
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "panel_icon": "mdi:music",
  "ports": {
    "3000/tcp": 3000
  },
  "ports_description": {
    "3000/tcp": "Web UI port"
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str"
  },
  "slug": "cloud_music",
  "startup": "services",
  "upstream": "4.8.2",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "4.8.2",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:3000]"
}
